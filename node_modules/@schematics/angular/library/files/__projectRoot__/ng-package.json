{
  "$schema": "<%= projectRoot.split('/').map(x => '..').join('/') %>/node_modules/ng-packagr/ng-package.schema.json",
  "dest": "<%= projectRoot.split('/').map(x => '..').join('/') %>/dist/<%= dasherize(packageName) %>",
  "deleteDestPath": false,
  "lib": {
    "entryFile": "src/<%= entryFile %>.ts"
  }
}